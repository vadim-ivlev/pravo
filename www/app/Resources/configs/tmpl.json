{
	"name": "tmpl",
	"subTasks": [
		{
			"name": "index",
			"dest": "/src/JuristBundle/Resources/views",
			"src": {
				"include": [
					"/app/Resources/public/pages/data/index.js"
				]
			},
			"plugins": [
				{
					"name": "gulp-rgswig",
					"options": {
						"param": {
							"compileType": "data"
						}
					}
				},
				{
				  "name": "gulp-minify-html",
				  "options": {
					"conditionals": true,
					"comments": true,
					"quotes": true
				  }
				}
			]
		}
	]
}