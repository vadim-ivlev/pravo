{
	"name": "projects:lada:includes:loadarray",
	"subTasks": [
		{
			"name": "article",
			"dest": "/dest/templates/custom/projects/lada/templates/loadarray",
			"src": {
				"include": [ "/src/pages/includes/route.html" ]
			},
			"plugins": [
				{
					"name": "gulp-rgswig",
					"options": {
						"data": {
							"type": "loadarray",
							"name": "b-news",
							"tmplFile": "b-news.swig",
							"info": "{{# internal_tags}}{{.}}{{/ internal_tags}}"
						}
					}
				},
				{
					"name": "gulp-minify-html",
					"options": {
						"conditionals": true,
						"comments": true,
						"quotes": true
					}
				},
				{
					"name": "gulp-rename",
					"options": {
						"basename": "article",
						"extname": ".ihtml"
					}
				}
			]
		}
	]
}