{
  "name": "includes:xml",
  "subTasks": [
	{
		"name": "b-feed",
		"dest": "/dest/templates/includes",
		"src": {
			"include": [ "/src/pages/includes/route.html" ]
		},
		"plugins": [
			{
				"name": "gulp-rgswig",
				"options": {
					"data": {
						"type": "xml",
						"name": "b-feed",
						"swigFile": "b-feed.swig",
						"tmplFile": "b-xml.swig"
					}
				}
			},
			{
				"name": "gulp-rename",
				"options": {
					"basename": "b-feed",
					"extname": ".xml"
				}
			}
		]
	},
	{
		"name": "b-news",
		"dest": "/dest/templates/includes",
		"src": {
			"include": [ "/src/pages/includes/route.html" ]
		},
		"plugins": [
			{
				"name": "gulp-rgswig",
				"options": {
					"data": {
						"type": "xml",
						"name": "b-news",
						"swigFile": "b-news.swig",
						"tmplFile": "b-xml.swig"
					}
				}
			},
			{
				"name": "gulp-rename",
				"options": {
					"basename": "b-news",
					"extname": ".xml"
				}
			}
		]
	}
  ]
}