{
  "name": "conf-for-json",
  "subTasks": [
	{
		"name": "index",
		"dest": "/dest/templates/includes/conf_json",
		"src": {
			"include": [ "/src/pages/includes/route.html" ]
		},
		"plugins": [
			{
				"name": "gulp-rgswig",
				"options": {
					"data": {
						"name": "index",
						"tmplFile": "conf-for-json.swig",
						"confJson": "art_id doc_id has_photo has_video add_subtitle title link_title uplicate_link_title datetime uri images_medium__length images_medium images_xlarge__length images_xlarge"
					}
				}
			},
			{
				"name": "gulp-minify-html",
				"options": {
					"conditionals": true,
					"comments": true,
					"quotes": true
				}
			},
			{
				"name": "gulp-rename",
				"options": {
					"basename": "index",
					"extname": ""
				}
			}
		]
	}
  ]
}