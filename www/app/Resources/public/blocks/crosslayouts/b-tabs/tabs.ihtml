<div class="b-tabs">
    <div class="b-tabs__item b-tabs__item_underline"
        style="left: {{left}}px; width: {{width}}px;"></div>

    {{#items: i}}
        <div class="b-tabs__item {{item.class ? item.class : 'b-tab__item_' + i}}"
             on-click="select: {{.}}">

            <div class="b-tabs__title {{i === selected ? 'b-tab__title_selected' : ''}}">{{{title}}}</div>
        </div>
    {{/itmes}}
</div>