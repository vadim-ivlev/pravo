	{
		"name": "b-media",
		"dest": "/dest/templates/includes",
		"src": {
			"include": [ "/src/pages/includes/route.html" ]
		},
		"plugins": [
			{
				"name": "gulp-rgswig",
				"options": {
					"data": {
						"type": "include",
						"name": "b-media",
						"tmplFile": "b-media.swig",
						"object": "objects__",
						"info": "{{# internal_tags}}{{.}}{{/ internal_tags}}"
					}
				}
			},
			{
				"name": "gulp-minify-html",
				"options": {
					"conditionals": true,
					"comments": true,
					"quotes": true
				}
			},
			{
				"name": "gulp-rename",
				"options": {
					"basename": "b-media"
				}
			}
		]
	},
